		<settings accessibilityCompliant="true" bootstrapMode="Bootstrap3" doctypeMode="Html5" rightToLeft="false" checkReferencesToExternalScripts="true" protectControlState="true" ieCompatibilityVersion="edge" />
		<errors callbackErrorRedirectUrl="" />
	</devExpress>
	<appSettings>
		<add key="vs:EnableBrowserLink" value="false" />
		<add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
